  filter(child_birth_year == 2018 | child_birth_year == 2019 | child_birth_year == 2020 | child_birth_year== 2021 | 
           child_birth_year == 2022 | child_birth_year == 2023 | child_birth_year == 2024)

table (ih_child_0_10yr$child_birth_year)


