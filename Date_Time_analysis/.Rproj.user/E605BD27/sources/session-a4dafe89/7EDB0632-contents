  select(dup_dup, dupflu, flu_vac, id, child_dob, service.date, dob, time_diff_weeks, mean_closer, time_diff_years, child_birth_year, pregstat42wks, pregstat_37wks)  # looks good

rm(temp_Flu_preg_cohort)


